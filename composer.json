    "description": "Add tagging to notifications going out of a Silverstripe project",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "nsw",
        "tagging",
        "tags",
        "notifications"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "James Ellis",
            "homepage": "https://dpc.nsw.gov.au",
            "role": "Developer"
        }
    ],
    "require": {
        "silverstripe/tagfield" : "^2",
        "silverstripe/taxonomy" : "^2"
    },
    "suggest" : {
        "silverstripe/userforms" : "Install this module to get tagging of notifications from a user form"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "friendsofphp/php-cs-fixer": "^2.18"
    }
